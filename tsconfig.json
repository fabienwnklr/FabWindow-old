{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "strictPropertyInitialization": false
  },
  "include": ["./lib"],
  "typedocOptions": {
    "entryPoints": ["lib/constant.ts", "lib/FabModal.ts", "lib/FabModalManager.ts"],
    "out": "website/docs/api/"
  }
}
